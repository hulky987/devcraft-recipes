// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model UserLocal {
  id    Int     @id @default(autoincrement())
  name  String
  email String
  password String
}

model UserGithub {
  id    Int     @id @default(autoincrement())
  name  String
  email String
  password String
}
